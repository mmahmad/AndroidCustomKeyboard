<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="9.5%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="49" android:keyLabel="1" android:keyEdgeFlags="left" android:horizontalGap="0.5%p"/>
        <Key android:codes="50" android:keyLabel="2" android:horizontalGap="0.5%p"/>
        <Key android:codes="51" android:keyLabel="3" android:horizontalGap="0.5%p"/>
        <Key android:codes="52" android:keyLabel="4" android:horizontalGap="0.5%p"/>
        <Key android:codes="53" android:keyLabel="5" android:horizontalGap="0.5%p"/>
        <Key android:codes="54" android:keyLabel="6" android:horizontalGap="0.5%p"/>
        <Key android:codes="55" android:keyLabel="7" android:horizontalGap="0.5%p"/>
        <Key android:codes="56" android:keyLabel="8" android:horizontalGap="0.5%p"/>
        <Key android:codes="57" android:keyLabel="9" android:horizontalGap="0.5%p"/>
        <Key android:codes="48" android:keyLabel="0" android:keyEdgeFlags="right" android:horizontalGap="0.5%p"/>
    </Row>
    
    <Row>
        <Key android:codes="64" android:keyLabel="\@" android:keyEdgeFlags="left" android:horizontalGap="0.5%p"/>
        <Key android:codes="35" android:keyLabel="\#" android:horizontalGap="0.5%p"/>
        <Key android:codes="36" android:keyLabel="$" android:horizontalGap="0.5%p"/>
        <Key android:codes="37" android:keyLabel="%" android:horizontalGap="0.5%p"/>
        <Key android:codes="38" android:keyLabel="&amp;" android:horizontalGap="0.5%p"/>
        <Key android:codes="42" android:keyLabel="*" android:horizontalGap="0.5%p"/>
        <Key android:codes="45" android:keyLabel="-" android:horizontalGap="0.5%p"/>
        <Key android:codes="61" android:keyLabel="=" android:horizontalGap="0.5%p"/>
        <Key android:codes="40" android:keyLabel="(" android:horizontalGap="0.5%p"/>
        <Key android:codes="41" android:keyLabel=")" android:keyEdgeFlags="right" android:horizontalGap="0.5%p"/>
    </Row>
    
    <Row>
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift"
                android:keyWidth="15%p" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="left" android:horizontalGap="0.5%p"/>
        <Key android:codes="33" android:keyLabel="!"  android:horizontalGap="0.5%p"/>
        <Key android:codes="34" android:keyLabel="&quot;" android:horizontalGap="0.5%p"/>
        <Key android:codes="39" android:keyLabel="\'" android:horizontalGap="0.5%p"/>
        <Key android:codes="58" android:keyLabel=":" android:horizontalGap="0.5%p"/>
        <Key android:codes="59" android:keyLabel=";" android:horizontalGap="0.5%p"/>
        <Key android:codes="47" android:keyLabel="/"  android:horizontalGap="0.5%p"/>
        <Key android:codes="63" android:keyLabel="\?" android:horizontalGap="0.5%p"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
                android:keyWidth="15%p" android:keyEdgeFlags="right"
                android:isRepeatable="true" android:horizontalGap="0.5%p"/>
    </Row>
    
    <Row  android:rowEdgeFlags="bottom">

        <!--<Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done"-->
                <!--android:keyWidth="10%p" android:keyEdgeFlags="left" android:horizontalGap="0.5%p" />-->
        <Key android:codes="-101" android:keyIcon="@drawable/sym_keyboard_language_switch"
            android:keyWidth="8%p" android:horizontalGap="0.5%p" android:keyEdgeFlags="left" />

        <Key android:codes="-2" android:keyLabel="ABC" android:keyWidth="11%p" android:horizontalGap="0.5%p" />

        <Key android:codes="44" android:keyLabel=","
            android:keyWidth="10%p" android:horizontalGap="0.5%p"/>
        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->

        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="47%p"
                android:isRepeatable="true" android:horizontalGap="0.5%p"/>
        <Key android:codes="44" android:keyLabel="," android:keyWidth="13%p" android:horizontalGap="0.5%p" />
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
                android:keyWidth="8%p" android:keyEdgeFlags="right"
            android:horizontalGap="0.5%p"/>
    </Row>
</Keyboard>